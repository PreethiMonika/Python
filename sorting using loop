List1 = [10,20,30,40,50,60,70,80]
List2 = [5,15,25,35,45,55,65,75]

size_1 = len(List1) 
size_2 = len(List2)

result = [] 
a, b = 0, 0
  
while a < size_1 and b < size_2: 
    if List1[a] < List2[b]: 
      result.append(List1[a]) 
      a += 1
  
    else: 
      result.append(List2[b]) 
      b += 1
  
print(result)
